{
  "name": "sampler-api",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@sampler-api-db-user",
    "DB_PASSWORD": "@sampler-api-db-password",
    "DB_HOST": "@sampler-api-db-host",
    "DB_NAME": "@sampler-api-db-name"
  }
}
